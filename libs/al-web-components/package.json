    "storybook": "^7.6.17",
    "style-dictionary": "^3.9.2",
    "web-component-analyzer": "^2.0.0",
    "webpack-cli": "^5.1.4"
  },
  "resolutions": {
    "@lit/reactive-element": "^2.0.3",
    "react": "18.2.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
