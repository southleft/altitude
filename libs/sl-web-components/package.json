{
  "name": "sl-web-components",
  "version": "0.1.0",
  "license": "BSD-3-Clause",
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "start": "storybook dev -p 6006",
    "build": "del-cli dist && npm run build:icons && webpack && tsc && npm run build:custom-elements.json",
    "build-storybook": "storybook build",
    "build:tokens": "node ./styles/tokens-config.js",
    "build:icons": "node ./icons/icons-config.js",
    "build:custom-elements.json": "wca analyze \"components/*/*.ts\" --outFile custom-elements.json",
    "test": "test-storybook",
    "plop": "plop --plopfile plop/plop-config.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./dist/icons/*": "./dist/icons/*",
    "./dist/iconfonts/*": "./dist/iconfonts/*",
    "./dist/styles/*": "./dist/styles/*",
    "./styles/head.scss": "./styles/head.scss",
    "./dist/components/*": "./dist/components/*",
    "./dist/directives/*": "./dist/directives/*",
    "./components/*": "./components/*.ts"
  },
  "dependencies": {
    "date-fns": "^3.2.0",
    "lit": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-class-static-block": "^7.23.4",
    "@babel/plugin-transform-typescript": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@etchteam/storybook-addon-status": "^4.2.4",
    "@storybook/addon-a11y": "^7.6.10",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/blocks": "^7.6.7",
    "@storybook/jest": "^0.2.3",
    "@storybook/manager-api": "^7.6.10",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.6.10",
    "@storybook/web-components": "^7.6.7",
    "@storybook/web-components-webpack5": "^7.6.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.9.1",
    "datauri": "^4.1.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-storybook": "^0.6.15",
    "file-loader": "^6.2.0",
    "plop": "^4.0.1",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass-loader": "^14.0.0",
    "scale-that-svg": "^1.0.6",
    "storybook": "^7.6.7",
    "style-dictionary": "^3.9.2",
    "web-component-analyzer": "^2.0.0",
    "webfonts-generator": "^0.4.0",
    "webpack-cli": "^4.7.2"
  },
  "resolutions": {
    "@lit/reactive-element": "^2.0.3",
    "react": "18.2.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
